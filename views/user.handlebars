<div>

    <br>
    <h2>Your Personal Info</h2>
    <p>UserName: {{userName}}</p>
    <p>First Name: {{firstName}}</p>
    <p>Last Name: {{lastName}}</p>
    <br>
    <br>
    <hr id="hr-body">
    <h2>Your Friends List</h2>
    {{#each friendsList}}
    <p>
        <a href="/friendsList/{{this}}">{{this}}</a>
    </p>
    {{/each}}
    <br>
    <br>
    <h2>Add more Friends!</h2>
    <form class="" action="/addFriend" method="post">
        <textarea name="newFriend" placeholder="Input Friend's Github Username" rows="2" cols="25"></textarea>
        <br>
        <input type="submit" name="" value="Add Friend">
        <input type="reset" name="" value="Clear">
    </form>

</div>
